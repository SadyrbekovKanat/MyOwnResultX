<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

	<PropertyGroup>
		<Authors>KanatSadyrbekov</Authors>
		<PackageId>MyOwnResultX</PackageId>
		<Title>MyOwnResultX</Title>
		<Description>Library for custom Result objects.</Description>
		<PackageTags>x;myownresultx;result;results;fluent;fluentresults;operationresult;pattern</PackageTags>
		<PackageProjectUrl>https://github.com/SadyrbekovKanat/MyOwnResultX</PackageProjectUrl>
		<RepositoryUrl>https://github.com/SadyrbekovKanat/MyOwnResultX</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<AssemblyVersion>1.0.0</AssemblyVersion>
		<FileVersion>1.0.0</FileVersion>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
